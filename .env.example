# Neon Database Configuration
# Copy this file to .env.local for local development
# Add these environment variables to your Vercel project settings

# Your Neon database connection string
# Format: postgresql://username:password@hostname/database?sslmode=require
POSTGRES_URL=postgresql://your-username:your-password@your-neon-hostname/your-database-name?sslmode=require

# Example:
# POSTGRES_URL=postgresql://aplls_user:your_password@ep-cool-cloud-123456.us-east-1.aws.neon.tech/aplls_2026_db?sslmode=require

# Optional: Node environment (automatically set by Vercel)
NODE_ENV=production